{
  "compilerOptions": {
    "baseUrl": "./src",
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "module": "es6",
    "moduleResolution": "node",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": false,
  },
  "include": [
    "src"
  ]
}
